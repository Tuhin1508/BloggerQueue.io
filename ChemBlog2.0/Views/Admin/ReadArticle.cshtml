@model List<ChemBlog2._0.Models.AdminPanelEntity>;

<div class="container">
    <div class="row">
        <div class="col">
            <div class="card">
                <duv class="card-body" style="border-block-color:black">
                    <div class="shadow p-3 mb-5 bg-white rounded">
                        @Html.Raw(@ViewBag.title)
                    </div>
                </duv>
            </div>
        </div>
        <div class="col-4">
            <div class="card">
                <div class="card-body">
                    <div class="shadow-sm p-3 mb-5 bg-white rounded">
                        <form method="post" ,asp-action="ReadArticle">
                            <table class="table table-bordered">
                                <thead>
                                    @foreach (var item in Model)
                                    {
                                        <tr class="clickable-row" ,>
                                            <th scope="col">
                                                <input type="submit" name="Read+@item.Id" class="btn btn-light w-100" value=@item.Heading />
                                            </th>
                                        </tr>
                                    }
                                </thead>
                            </table>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
<div>